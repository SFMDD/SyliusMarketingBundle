{# FAQ #}
<script type="application/ld+json">
{
  "@context": "http://schema.org/",
  "@type": "FAQPage",
  "mainEntity": [
      {% for faq in faqs %}
      {
          "@type": "Question",
          "name": "{{ faq.question }}",
          "acceptedAnswer": {
              "@type": "Answer",
              "text": "{{ faq.answer }}"
          }
      }{% if not loop.last %},{% endif %}
      {% endfor %}
  ]
}
</script>