{% set code_error = settings.code_error %}
<script>
    tarteaucitron.user.gtagMore = function () {
        gtag('event', 'exception', {
            'description': 'Error '+'{{ code_error }}',
            'fatal': false
        });
    };
    (tarteaucitron.job = tarteaucitron.job || []).push('gtag');
</script>
